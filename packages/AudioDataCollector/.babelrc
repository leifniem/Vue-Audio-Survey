{
	"presets": [
		[
			"@babel/env",
			{
				"useBuiltIns": "usage",
				"targets": {
					"browsers": "last 2 versions, safari >= 7, ios_saf >= 9, chrome >= 52"
				},
				"loose": true
			}
		]
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-transform-classes"
	]
}
